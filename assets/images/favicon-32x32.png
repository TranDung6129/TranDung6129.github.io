<svg width="80" height="80" viewBox="0 0 300 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style>
      .logo-bg { fill: #7bb3a0; }
      .logo-atom { fill: none; stroke: #ffffff; stroke-width: 8; }
      .logo-nucleus { fill: #ffffff; }
      .logo-text { font-family: serif; font-size: 48px; font-weight: bold; fill: #4a4a4a; }
      .logo-text-small { font-family: serif; font-size: 24px; fill: #4a4a4a; }
    </style>
  </defs>
  
  <!-- Background circle -->
  <circle cx="150" cy="150" r="120" class="logo-bg"/>
  
  <!-- Atom structure -->
  <g transform="translate(150,150)">
    <!-- Electron orbits -->
    <ellipse cx="0" cy="0" rx="50" ry="15" class="logo-atom"/>
    <ellipse cx="0" cy="0" rx="50" ry="15" class="logo-atom" transform="rotate(60)"/>
    <ellipse cx="0" cy="0" rx="50" ry="15" class="logo-atom" transform="rotate(120)"/>
    
    <!-- Nucleus -->
    <circle cx="0" cy="0" r="8" class="logo-nucleus"/>
  </g>
</svg>
